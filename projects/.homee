function reset_homee_db {
  if [ ! -n "$1" ]; then
    echo "Enter a file to import"
  else
    user=root
    database=HOMEE_WAP_DB

    mysql --user="$user" --database="$database" --execute="DROP DATABASE $database; CREATE DATABASE $database;"
    mysql --user="$user" --database="$database" < "$1"
  fi
}

export APPLICATION_ENV=dev